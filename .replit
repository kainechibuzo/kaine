modules = ["python-3.11"]
[agent]
expertMode = true
integrations = ["python_log_in_with_replit:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Ultimate Bot"

[[workflows.workflow]]
name = "Ultimate Bot"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python ultimatest_bot.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[deployment]
deploymentTarget = "vm"
run = ["python", "ultimatest_bot.py"]
